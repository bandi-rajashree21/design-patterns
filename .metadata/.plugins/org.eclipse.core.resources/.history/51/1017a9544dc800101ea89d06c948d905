package com.demo;

public class SingleTon {
        public static void main(String[] args)
        {
        	Abc obj1=new Abc.getInstance();
        	Abc obj2=new Abc.getInstance;
        	
        }
        
        class Abc {
        	static Abc obj=new Abc();
        	private Abc()
        	{
        		
        	}
        	
        	public static Abc getInstance()
        	{
        		return obj;
        	}

}
