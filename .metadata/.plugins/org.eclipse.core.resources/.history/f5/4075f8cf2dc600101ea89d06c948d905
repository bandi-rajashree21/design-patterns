interface Component{
	void showPrice();
}

class Leaf implements Component{
    int price;
    String name;
	@Override
	public void showPrice() {
		// TODO Auto-generated method stub
		
	}
	
}

class Composite implements Component{
    
	List<Component>components=new ArrayList<>();
	
	public void addComponent(Component comp)
	{
		components.add(comp);
	}
	
	@Override
	public void showPrice() {
		// TODO Auto-generated method stub
		
	}
	
	
}